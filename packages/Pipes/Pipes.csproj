<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>DavidVollmers.Pipes</PackageId>
        <Authors>github@david.vollmers.org</Authors>
        <Description>Simplify complex processes by building pipes from small code units.</Description>
        <PackageProjectUrl>https://github.com/DavidVollmers/pipes</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\Pipes.Abstractions\Pipes.Abstractions.csproj" />
    </ItemGroup>

</Project>